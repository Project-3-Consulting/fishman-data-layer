.cart-design-2 {
  .icon-cart-header {
    display: block;

    i {font-size: 35px;display: block;}

    .cart-total {
      display: block;
      position: absolute;
      top: 0;
      width: 100%; 
      padding: 3px;
      font-size: 16px;
      font-weight: 600;
      text-align: center;

      .cart-qty {display: block;}
    }

    span {display: none;}
  }

  .cart-wrapper::before {right: 15px;}
}
{% endif %}
{% if settings.shopping_cart_design == '3' %}
.cart-design-3 {
  .icon-cart-header {
    height: auto;
    padding: 0;
    display: flex;
    align-items: center;
    top: -1px;
    i {
      font-size: 26px; 
      &::before { 
        margin: 0;
        content: '\e8ba';
      }
    }
    .cart-total {
      position: absolute;
      right: 18px;
      top: 3px;
      padding: 0;
      .cart-qty {
        display: block;
        width: 16px;
        height: 16px;
        border-radius: 16px !important;
        background-color: #ff5b5b;
        font-size: 10px;
        font-weight: 600;
        line-height: 16px;
        text-align: center;
        color:#fff;
      }
      span {display: none;}
    }
    &:after {
      content: "\e81c";
      font-family: Porto;
      font-size: 17px;
      margin-left: 15px;
    }
  }
  .cart-wrapper::before {right: 5px;}
}
{% endif %}
{% if settings.shopping_cart_design == '4' %}
.cart-design-4 {
  .icon-cart-header {
    white-space: nowrap;
    i {
      display: inline-block;
      font-size: 27px;
      vertical-align: middle;
      &::before {content: '\e878';}
    }
    .cart-total {
      display: inline-block;
      vertical-align: middle;

      .cart-qty {
        position: absolute;
        top: -4px;
        left: 22px;
        width: 15px;
        height: 15px;
        border-radius: 20px !important;
        background-color: #ff4052;
        color: #fff;
        font-family: 'Poppins';
        font-size: 11px;
        line-height: 16px;
        text-align: center;
      }
      span {
        font-family: 'Oswald';
        font-size: 15px;
        font-weight: 300;
        letter-spacing: .03em;
        text-transform: initial;
      }
    }
  }
}
{% endif %}
{% if settings.shopping_cart_design == '5' %}
.cart-design-5 {
  .icon-cart-header {
    display: inline-block;
    padding: 0 19px 0 0;
    i {
      display: inline-block;
      font-size: 33px;
      vertical-align: middle;
      &::before {content: '\e87f';}
    }
    .cart-total {
      display: block;
      position: absolute;
      top: 50%;
      width: 100%;
      margin-top: -8px;
      color: #fff;
      font-size: 14px;
      font-weight: 600;
      text-align: center;
      .cart-qty {
        display: block;
        position: absolute;
        top: -10px;
        right: 19px;
        width: 16px;
        height: 16px;
        border-radius: 20px !important;
        background-color: #ed5348;
        color: #fff;
        font-size: 10px;
        line-height: 16px;
        text-align: center;
      }
      span {display: none;}
    }
    &::after {
      position: absolute;
      top: 50%;
      right: 0;
      transform: translateY(-50%);
      -webkit-transform: translateY(-50%);
      font-family: 'fontawesome';
      content: '\f107';
    }
  }
}
{% endif %}
