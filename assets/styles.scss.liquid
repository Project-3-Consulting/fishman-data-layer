$theme-color: {{settings.theme_color}};
* {
  margin: 0;
  padding: 0;
}

html {
  font-size: 16px;
  font-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
  padding: 0 !important;
  overflow-x: hidden;
} 
.hide {display: none;}
.show {display: block;}
body {
  line-height: 1;
  text-align: left;
  &.boxed { 
    background-color: #000; 
    padding: 25px 0;
    .wrapper { 
      .page { 
        margin-right: auto;
        margin-left: auto;
        padding-left: 20px;
        padding-right: 20px;
        @media (min-width: 768px) {
          max-width: 740px;
        }
        @media (min-width: 992px) {
          max-width: 980px;
        }
        @media (min-width: 1160px) {
          max-width: 1160px; 
        }
      }
    }
    .container {
      padding: 0;
    }
  } 
}
