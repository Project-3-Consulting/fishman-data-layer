{% if settings.shopping_cart_design == '1' %}
.cart-design-1 {
  padding: 6px 7px;

  .icon-cart-header {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
    border-radius: 2px;
